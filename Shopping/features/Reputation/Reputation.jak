public abstract class Reputation {
	
	private long id;
	private String valor;
	
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public String getValor() {
		return valor;
	}
	public void setValor(String valor) {
		this.valor = valor;
	}
	
	public abstract void avaliar(String valor);
	
	
}